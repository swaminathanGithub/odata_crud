<mvc:View controllerName="odatacrud.controller.odata_crud"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
      <Table id="idProducts" items="{/results}">
        <columns>
          
          <Column id="_IDGenColumn" >
            <Text id="_IDGenText" text="ID"/>
          </Column>
          <Column id="_IDGenColumn2" >
            <Text id="_IDGenText2" text="Name"/>
          </Column>
          <Column id="_IDGenColumn3" >
            <Text id="_IDGenText3" text="Price"/>
          </Column>
          <Column id="_IDGenColumn4" >
            <Text id="_IDGenText4" text="Rating"/>
          </Column>
        </columns>

        <items>
          <ColumnListItem id="_IDGenColumnListItem1" >
            <cells>
              <Text id="_IDGenText1" text="{ID}"/>
              <Text id="_IDGenText5" text="{Name}"/>
              <Text id="_IDGenText6" text="{Price}"/>
              <Text id="_IDGenText7" text="{Rating}"/>
            </cells>
          </ColumnListItem>    
        </items>
      </Table>  
    </Page>
</mvc:View>